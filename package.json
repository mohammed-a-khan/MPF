{
  "name": "cs-test-automation-framework",
  "version": "1.0.0",
  "description": "CS Test Automation Framework - Enterprise-grade zero-code BDD test automation with AI-powered self-healing",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "postinstall": "playwright install",
    "test": "cross-env TS_NODE_PROJECT=./tsconfig.json ts-node src/index.ts",
    "test:akhan": "cross-env TS_NODE_PROJECT=./tsconfig.json ts-node src/index.ts --env=dev --project=akhan",
    "test:akhan:headed": "cross-env TS_NODE_PROJECT=./tsconfig.json ts-node src/index.ts --env=dev --project=akhan --feature=test/akhan/features/akhan-login-navigation.feature --headless=false",
    "test:akhan:headless": "cross-env TS_NODE_PROJECT=./tsconfig.json ts-node src/index.ts --env=dev --project=akhan --feature=test/akhan/features/akhan-login-navigation.feature --headless=true",
    "test:akhan:debug": "cross-env TS_NODE_PROJECT=./tsconfig.json ts-node src/index.ts --env=dev --project=akhan --feature=test/akhan/features/akhan-login-navigation.feature --headless=false --debug=true",
    "test:akhan:parallel": "cross-env TS_NODE_PROJECT=./tsconfig.json ts-node src/index.ts --env=dev --project=akhan --feature=test/akhan/features/akhan-login-navigation.feature --headless=true --parallel=true --workers=4",
    "test:akhan:report": "npm run test:akhan -- --report=true",
    "test:akhan:tags": "npm run test:akhan -- --tags",
    "test:akhan:rerun": "npm run test:akhan -- --rerun=true",
    "test:akhan:smoke": "npm run test:akhan -- --tags=@smoke",
    "test:akhan:regression": "npm run test:akhan -- --tags=@regression",
    "test:crru": "cross-env TS_NODE_PROJECT=./tsconfig.json ts-node src/index.ts --project=crru",
    "test:crru:headed": "cross-env TS_NODE_PROJECT=./tsconfig.json ts-node src/index.ts --project=crru --headless=false",
    "test:feature": "cross-env TS_NODE_PROJECT=./tsconfig.json ts-node src/index.ts",
    "clean": "rimraf dist reports logs screenshots downloads videos traces",
    "clean:reports": "rimraf reports",
    "clean:logs": "rimraf logs",
    "clean:media": "rimraf screenshots videos traces",
    "build": "tsc",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"features/**/*.feature\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"features/**/*.feature\""
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@ffprobe-installer/ffprobe": "^2.1.2",
    "@playwright/browser-chromium": "^1.40.1",
    "@playwright/browser-firefox": "^1.40.1",
    "@playwright/browser-webkit": "^1.40.1",
    "@playwright/test": "^1.40.1",
    "@types/archiver": "^6.0.3",
    "@types/node": "^20.10.5",
    "@types/ws": "^8.18.1",
    "archiver": "^7.0.1",
    "axios": "^1.9.0",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.2.0",
    "csv-parse": "^5.0.0",
    "csv-stringify": "^6.5.2",
    "dotenv": "^16.3.1",
    "fluent-ffmpeg": "^2.1.2",
    "glob": "^10.3.10",
    "jsonpath": "^1.1.1",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.16.0",
    "mysql2": "^3.14.1",
    "oracledb": "^6.8.0",
    "pg": "^8.16.0",
    "playwright": "^1.40.1",
    "redis": "^5.1.1",
    "reflect-metadata": "^0.2.2",
    "sharp": "^0.33.0",
    "tedious": "^18.6.1",
    "ws": "^8.18.2",
    "xlsx": "^0.18.0",
    "xml2js": "^0.6.2",
    "xmldom": "^0.6.0",
    "xpath": "^0.0.34"
  },
  "devDependencies": {
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/jsonpath": "^0.2.4",
    "@types/mssql": "^9.1.7",
    "@types/oracledb": "^6.6.0",
    "@types/pg": "^8.15.4",
    "@types/sax": "^1.2.7",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@xmldom/xmldom": "^0.9.8",
    "copyfiles": "^2.4.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "mkdirp": "^3.0.1",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "ts-node-register": "^1.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  },
  "keywords": [
    "test-automation",
    "bdd",
    "playwright",
    "ai",
    "self-healing",
    "zero-code",
    "cucumber",
    "gherkin",
    "api-testing",
    "database-testing",
    "ui-testing",
    "typescript",
    "enterprise",
    "ado-integration",
    "saucedemo",
    "demo",
    "professional"
  ],
  "author": "CS Test Automation Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cs-automation/cs-test-automation-framework.git"
  },
  "bugs": {
    "url": "https://github.com/cs-automation/cs-test-automation-framework/issues"
  },
  "homepage": "https://github.com/cs-automation/cs-test-automation-framework#readme",
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.feature": [
      "prettier --write"
    ]
  }
}
